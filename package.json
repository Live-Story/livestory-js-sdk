{
  "name": "livestory-js-sdk",
  "version": "1.0.0-rc.1",
  "description": "JavaScript SDK for Live Story's Delivery API",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "unpkg": "./dist/index.umd.js",
  "jsdelivr": "./dist/index.umd.js",
  "types": "./dist/types/entry.esm.d.ts",
  "author": "Live Story",
  "license": "MIT",
  "source": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Live-Story/livestory-js-sdk.git"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types/entry.esm.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist",
    "src",
    "tests"
  ],
  "scripts": {
    "lint": "eslint --max-warnings=0 './src/**/*.{ts,js}'",
    "prettier": "prettier . --write",
    "build": "node vite.build.mjs && tsc",
    "demo": "vite serve playground",
    "test": "pnpm run test:unit:ci && pnpm run test:e2e",
    "test:unit:ci": "vitest run",
    "test:unit:ui": "vitest --ui",
    "test:e2e": "vitest run -c vitest.config.e2e.ts",
    "prepare": "npm run build",
    "coverage": "vitest run --coverage"
  },
  "keywords": [
    "livestory",
    "sdk"
  ],
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@eslint/js": "^9.18.0",
    "@tsconfig/recommended": "^1.0.3",
    "@types/node": "^22.10.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.1.8",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.0.0",
    "globals": "^15.14.0",
    "husky": "^9.0.6",
    "isomorphic-fetch": "^3.0.0",
    "prettier": "^3.3.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.6",
    "vite-plugin-banner": "^0.8.1",
    "vitest": "^3.2.4"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "dependencies": {
    "kolorist": "^1.8.0",
    "vite-plugin-dts": "^4.5.0"
  },
  "bugs": {
    "url": "https://github.com/Live-Story/livestory-js-sdk/issues"
  },
  "homepage": "https://github.com/Live-Story/livestory-js-sdk#readme",
  "directories": {
    "test": "tests"
  }
}
